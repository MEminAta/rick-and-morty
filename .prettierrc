{
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "printWidth": 1500
      }
    }
  ],
  "trailingComma": "none",
  "printWidth": 120,
  "singleQuote": false,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "bracketSameLine": true
}
